{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"env": {
		"browser": true,
		"node": true
	},
	"plugins": [
		"react",
		"react-hooks",
		"@typescript-eslint",
		"import",
		"unused-imports"
	],
	"rules": {
		"unused-imports/no-unused-imports-ts": ["warn"],
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": "off"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
